<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme.Aroundrmu" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_aroundrmu</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Aroundrmu</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Aroundrmu</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Aroundrmu</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Aroundrmu</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Aroundrmu</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_aroundrmu</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_aroundrmu</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Aroundrmu</item>
        
        
    </style>

    <style name="ActionBar.Solid.Aroundrmu" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_aroundrmu</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_aroundrmu</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_aroundrmu</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Aroundrmu</item>
    </style>

    <style name="ActionBar.Transparent.Aroundrmu" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_aroundrmu</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Aroundrmu</item>
    </style>

    <style name="PopupMenu.Aroundrmu" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_aroundrmu</item>	
    </style>

    <style name="DropDownListView.Aroundrmu" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_aroundrmu</item>
    </style>

    <style name="ActionBarTabStyle.Aroundrmu" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_aroundrmu</item>
        <item name="android:textSize">25dp</item>
    </style>

    <style name="DropDownNav.Aroundrmu" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_aroundrmu</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_aroundrmu</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_aroundrmu</item>
    </style>
    
    <style name="ProgressBar.Aroundrmu" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_aroundrmu</item>
    </style>
    
    <style name="ActionButton.CloseMode.Aroundrmu" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_aroundrmu</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Aroundrmu.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Aroundrmu</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Aroundrmu</item>
    </style>
    
</resources>
